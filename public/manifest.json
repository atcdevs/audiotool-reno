{
	"name": "Audiotool Reno",
	"description": "Provides additional UI/UX features for audiotool.com",
	"version": "0.1.0",
	"manifest_version": 3,
	"icons": {
		"16": "icons/x16.png",
		"48": "icons/x48.png",
		"128": "icons/x128.png"
	},
	"permissions": ["storage"],
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "background.js"
	}
}
